<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.mvddoc" version="1.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Mvddoc</name>
    <description>
        Mvddoc Doctors App
    </description>
    <author email="rahulbaruah@xcraft.co" href="http://xcraft.co">
        Xcraftonline Pvt Ltd
    </author>
    <content src="index.html" />
    <icon src="www/img/icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" launch-external="yes" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
